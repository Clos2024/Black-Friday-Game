/// @description Set waves based off time
// You can write your code in this editor

//If clock strikes 2 set wave to 1
if(global.hours = 2){
	wave = 1;
}
//If clock strikes 4 set wave to 2
if(global.hours = 4){
	wave = 2;
}
//If clock strikes 6 set wave to 3
if(global.hours = 6){
	wave = 3;
}
//If clock strikes 8 set wave to 4
if(global.hours = 8){
	wave = 4;
}

